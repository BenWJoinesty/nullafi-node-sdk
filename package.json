{
  "name": "nullafi-node-sdk",
  "version": "1.0.0",
  "description": "Official Node.js SDK for Nullafi APIs",
  "main": "./lib/nullafi-node-sdk.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "test": "mocha ./lib/**/*.spec.js",
    "lint": "eslint ./lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joinesty/nullafi-node-sdk.git"
  },
  "keywords": [
    "nullafi",
    "api",
    "sdk"
  ],
  "author": "Joinesty Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joinesty/nullafi-node-sdk/issues"
  },
  "homepage": "https://github.com/Joinesty/nullafi-node-sdk#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "sinon": "^7.3.1"
  },
  "dependencies": {
    "sjcl": "^1.0.8"
  }
}
