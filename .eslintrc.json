{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", 150],
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "indent": ["error", 4]
    }
}